{
    "name": "prismarinejs.github.io",
    "version": "1.0.0",
    "description": "PrismarineJS website build with Next.js",
    "private": true,
    "homepage": "https://prismarinejs.github.io/",
    "dependencies": {
        "@types/node": "18.15.11",
        "@types/react": "18.0.32",
        "@types/react-dom": "18.0.11",
        "next": "13.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-icons": "^4.8.0",
        "sass": "^1.59.2",
        "typescript": "5.0.3"
    },
    "devDependencies": {
        "prettier": "^2.8.6",
        "eslint": "8.37.0",
        "eslint-config-next": "13.2.4"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "export": "next export",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch --passWithNoTests",
        "test:ci": "npm run test -- --coverage --passWithNoTests",
        "update-snapshot": "npm run test -- --updateSnapshot"
    }
}
